{% extends 'base.html.twig' %}

{% block title %}Hello Homie!{% endblock %}

{% block body %}

<div id="carousel_auto" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://www.basketvision.fr/18528-thickbox_default/set-de-10-cones-d-entrainement-basket-avec-grip.jpg" class="d-block" data-bs-interval="1500" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://www.aps.dz/media/k2/items/cache/26a1f0c37d60f406a3cfd7089935d8a5_M.jpg" class="d-block" data-bs-interval="1500" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://images.lanouvellerepublique.fr/image/upload/t_1020w/5f333e29c80f7d27198b462c.jpg" class="d-block" data-bs-interval="1500" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carousel_auto" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carousel_auto" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>

    

    

{% endblock %}
